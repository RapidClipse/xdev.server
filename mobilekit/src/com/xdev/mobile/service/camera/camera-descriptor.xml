<?xml version="1.0" encoding="UTF-8"?>
<mobile-service xmlns="http://rapidclipse.com/mobile-service/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://rapidclipse.com/mobile-service/1.0.0 http://rapidclipse.com/xsd/mobile-service-1.0.0.xsd">
	<android>
		<config>
			<feature name="Camera">
				<param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
			</feature>
		</config>
		<manifest>
            <permission>CAMERA</permission>
			<permission>WRITE_EXTERNAL_STORAGE</permission>
		</manifest>
	</android>
	<ios>
		<config>
			<feature name="Camera">
				<param name="ios-package" value="CDVCamera" />
			</feature>
			<preference name="CameraUsesGeolocation" value="false" />
		</config>
	</ios>
	<windows>
		<config>
			<feature name="Camera">
				<param name="wp-package" value="Camera" />
			</feature>
		</config>
		<manifest>
			<permission>ID_CAP_ISV_CAMERA</permission>
			<permission>ID_CAP_MEDIALIB_PHOTO</permission>
		</manifest>
	</windows>
</mobile-service>