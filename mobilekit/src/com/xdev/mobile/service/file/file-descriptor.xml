<?xml version="1.0" encoding="UTF-8"?>
<mobile-service xmlns="http://rapidclipse.com/mobile-service/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://rapidclipse.com/mobile-service/1.0.0 http://rapidclipse.com/xsd/mobile-service-1.0.0.xsd">
	<android>
		<config>
			<feature name="File" >
                <param name="android-package" value="org.apache.cordova.file.FileUtils"/>
                <param name="onload" value="true" />
            </feature>
		</config>
		<manifest>
			<permission>WRITE_EXTERNAL_STORAGE</permission>
		</manifest>
	</android>
	<ios>
		<config>
			<feature name="File">
                <param name="ios-package" value="CDVFile" />
                <param name="onload" value="true" />
            </feature>
		</config>
	</ios>
	<windows>
		<config>
			<feature name="File">
                <param name="wp-package" value="File"/>
            </feature>
		</config>
	</windows>
</mobile-service>