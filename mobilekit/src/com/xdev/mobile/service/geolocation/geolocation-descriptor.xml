<?xml version="1.0" encoding="UTF-8"?>
<mobile-service xmlns="http://rapidclipse.com/mobile-service/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://rapidclipse.com/mobile-service/1.0.0 http://rapidclipse.com/xsd/mobile-service-1.0.0.xsd">
	<android>
		<config>
			<feature name="Geolocation">
				<param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
			</feature>
		</config>
		<manifest>
			<permission>ACCESS_COARSE_LOCATION</permission>
			<permission>ACCESS_FINE_LOCATION</permission>
		</manifest>
	</android>
	<ios>
		<config>
			<feature name="Geolocation">
				<param name="ios-package" value="CDVLocation" />
			</feature>
		</config>
	</ios>
	<windows>
		<manifest>
			<permission>ID_CAP_LOCATION</permission>
		</manifest>
	</windows>
</mobile-service>