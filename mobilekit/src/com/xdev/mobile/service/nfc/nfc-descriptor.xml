<?xml version="1.0" encoding="UTF-8"?>
<mobile-service xmlns="http://rapidclipse.com/mobile-service/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://rapidclipse.com/mobile-service/1.0.0 http://rapidclipse.com/xsd/mobile-service-1.0.0.xsd">
	<android>
		<config>
			<feature name="NfcPlugin">
				<param name="android-package" value="com.chariotsolutions.nfc.plugin.NfcPlugin" />
				<param name="onload" value="true" />
			</feature>
		</config>
		<manifest>
			<permission>NFC</permission>
			<feature>android.hardware.nfc</feature>
		</manifest>
	</android>
	<ios>
	</ios>
	<windows>
		<config>
			<feature name="NfcPlugin">
				<param name="wp-package" value="NfcPlugin" />
			</feature>
		</config>
		<manifest>
			<permission>ID_CAP_PROXIMITY</permission>
		</manifest>
	</windows>
</mobile-service>